{
  "name": "parcellab-js-plugin",
  "version": "1.0.0",
  "description": "JavaScript plugin used to retrieve and display trackings from parcelLab on any given webpage.",
  "main": "src/index.js",
  "scripts": {
    "start": "parcel test/index.html -p 4000",
    "build": "parcel build src/index.js --no-source-maps",
    "clean": "rm -rf .cache && rm -rf dist",
    "deploy": "npm run clean && npm run build && make deploy && make deploy-v3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/parcelLab/parcelLab-js-plugin.git"
  },
  "keywords": [
    "parcellab",
    "trackings"
  ],
  "author": "Julian Krenge, Oscar Reyes, Andrej Fritz",
  "license": "MIT",
  "homepage": "https://www.parcellab.com",
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "node-sass": "^4.9.2",
    "parcel-bundler": "^1.9.7"
  },
  "dependencies": {
    "cash-dom": "^1.3.4",
    "nanohtml": "^1.2.4",
    "nanomorph": "^5.1.3",
    "nanostore": "^1.2.0"
  }
}
